{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-extra-semi": 0,
        "indent": 0,
        "eol-last": 0,
        "semi": 0,
        "space-before-function-paren": 0,
        "spaced-comment": 0,
        "object-curly-spacing": 0,
        "space-in-parens": 0
    }
}
